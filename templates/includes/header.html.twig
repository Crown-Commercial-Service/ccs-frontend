<header class="{% block headerclasses %}header-global{% endblock %}" {% block headerstyles %}{% endblock %}>
    <div class="ccs-width-container">

        {% block bannerarea %}
            <div class="govuk-phase-banner">
                <p class="govuk-phase-banner__content">
                    <strong class="govuk-tag govuk-phase-banner__content__tag ">
                        beta </strong> <span class="govuk-phase-banner__text">
      This is a new service â€“ your
      <a class="govuk-link" href="https://www.smartsurvey.co.uk/s/QWDZX/">feedback</a> will help us to improve it.
    </span>
                </p>
            </div>
        {% endblock %}

        <div class="nav-wrapper">

            <a href="/" class="logo"><img src="/assets/images/CCS_WHITE_SML_AW.png" alt="Go to the CCS homepage">
            </a>

            <button class="global-navigation__toggler" data-toggler="nav">
                <span>Menu</span> <span class="icon">
<svg width="16" height="13" xmlns="http://www.w3.org/2000/svg"><g stroke="currentColor" stroke-width="2" fill="none" fill-rule="evenodd" stroke-linecap="square"><path d="M14.5 1.12H1.464M14.5 6.12H1.464M14.5 11.24H1.464"/></g></svg>
        </span>
            </button>

            {% block mainnavigation %}
            <nav class="global-navigation" data-toggle="nav" data-app-uri="{{ app.request.uri }}" data-app-pathinfo="{{ app.request.pathinfo }}">

                {{ render(controller(
                    'App\\Controller\\MenuController::menu',
                    { 'id': 22, 'templatePath': 'menus/secondary-menu.html.twig', 'currentPath': app.request.pathinfo }
                )) }}

                {{ render(controller(
                    'App\\Controller\\MenuController::menu',
                    { 'id': 21, 'templatePath': 'menus/primary-menu.html.twig', 'currentPath': app.request.pathinfo }
                )) }}

            </nav>

            {% endblock %}
        </div>

        {% block featuredcontent %}{% endblock %}

    </div>
</header>
