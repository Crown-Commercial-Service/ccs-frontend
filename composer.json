{
    "name": "crown-commercial-service/ccs-frontend",
    "description": "Crown Commercial Service front-end website",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.272",
        "ccs/strata-frontend": "dev-upgrade-php8",
        "rollbar/rollbar-php-symfony-bundle": "4.0.0",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^5.4.0",
        "symfony/config": "^5.4.0",
        "symfony/dotenv": "^5.4.0",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^5.4.0",
        "symfony/http-client": "^5.4.0",
        "symfony/http-foundation": "^5.4.0",
        "symfony/http-kernel": "^5.4.0",
        "symfony/mime": "^5.4",
        "symfony/twig-bundle": "^5.4.0",
        "symfony/yaml": "^5.4.0",
        "toflar/psr6-symfony-http-cache-store": "^3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "symfony/*": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4.0"
        }
    },
    "require-dev": {
        "behat/mink": "^1.10",
        "behat/mink-zombie-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.7",
        "overtrue/phplint": "5.4.0",
        "rector/rector": "^2.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/debug-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^5.4.0"
    }
}
